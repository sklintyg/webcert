{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "VERSION": {
      "type": "string"
    },
    "BUILD_NUMBER": {
      "type": "string"
    },
    "PP_HOST": {
      "type": "string"
    },
    "DASHBOARD_URL": {
      "type": "string"
    },
    "JS_MINIFIED": {
      "type": "boolean"
    },
    "CGI_FUNKTIONSTJANSTER_IDP_URL": {
      "type": "string"
    },
    "SAKERHETSTJANST_IDP_URL": {
      "type": "string"
    },
    "WEBCERT_USER_SURVEY_URL": {
      "type": "string"
    },
    "WEBCERT_USER_SURVEY_VERSION": {
      "type": "string"
    },
    "WEBCERT_USER_SURVEY_DATE_TO": {
      "type": "string"
    },
    "WEBCERT_USER_SURVEY_DATE_FROM": {
      "type": "string"
    },
    "BANNERS": {
      "type": "array",
      "minItems": 0,
      "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "id": {
            "type": "string"
          },
          "createdAt": {
            "type": "string"
          },
          "displayFrom": {
            "type": "string"
          },
          "displayTo": {
            "type": "string"
          },
          "message": {
            "type": "string"
          },
          "application": {
            "type": "string"
          },
          "priority": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "createdAt",
          "displayFrom",
          "displayTo",
          "message",
          "application",
          "priority"
        ]
      }
    }
  },
  "required": [
    "VERSION",
    "BUILD_NUMBER",
    "PP_HOST",
    "DASHBOARD_URL",
    "JS_MINIFIED",
    "CGI_FUNKTIONSTJANSTER_IDP_URL",
    "SAKERHETSTJANST_IDP_URL",
    "BANNERS"
  ]
}
